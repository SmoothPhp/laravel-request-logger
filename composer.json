{
    "name": "smoothphp/laravel-request-logger",
    "description": "Logs request data to S3, Mongo, etc",
    "type": "library",
    "keywords": [
        "smoothphp",
        "request logger",
        "laravel"
    ],
    "homepage": "https://github.com/smoothphp/laravel-request-logger",
    "require": {
        "php": ">=7.0",
        "illuminate/bus": "^5.3",
        "illuminate/routing": "^5.3",
        "illuminate/queue": "^5.3",
        "illuminate/support": "^5.3"
    },
    "require-dev": {
        "ext-mongodb": "*",
        "jenssegers/mongodb": "^3.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Crocker",
            "email": "jordan@jcrocker.uk",
            "homepage": "https://jcrocker.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "SmoothPhp\\RequestLogger\\": "src"
        }
    },
    "suggest": {
        "ext-mongo": "Required to use the Mongo driver",
        "jenssegers/mongodb": "Required to use the Mongo driver"
    },
    "minimum-stability": "stable"
}
